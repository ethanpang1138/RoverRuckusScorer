<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ethanp1138.roverruckusscorerapp">

    <application
        android:allowBackup="true"
        android:icon="@drawable/scoring_app_launcher_icon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/scoring_app_launcher_icon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".StartMenuActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".AutonScoringActivity" />
        <activity android:name=".TeleOpScoringActivity"></activity>
    </application>

</manifest>